<?xml version="1.0" encoding="utf-8"?>
<!-- Secondary tutorial button (Skip, Cancel, etc.) -->
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/tutorial_button_ripple_secondary">
    
    <item>
        <shape>
            <solid android:color="@color/tutorial_secondary" />
            <corners android:radius="8dp" />
            <stroke
                android:width="1dp"
                android:color="@color/tutorial_border" />
        </shape>
    </item>
    
</ripple>
